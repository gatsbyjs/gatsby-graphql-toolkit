{
  "name": "gatsby-graphql-source-toolkit",
  "version": "0.5.0",
  "description": "A toolkit designed to simplify data sourcing from a remote GraphQL API into Gatsby.",
  "author": "Vladimir Razuvaev <vladimir@gatsbyjs.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rimraf dist/* && tsc",
    "prepublishOnly": "npm run build",
    "format": "prettier --list-different --write ./src"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "gatsby",
    "graphql"
  ],
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "p-queue": "^6.4.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.25",
    "gatsby": "^2.23.12",
    "graphql": "^14.6.0",
    "jest": "^26.1.0",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "gatsby": "^2.0.15",
    "graphql": "^14.0.0 || ^15.0.0"
  }
}
