{
  "name": "gatsby-graphql-source-toolkit",
  "version": "2.0.3",
  "description": "A toolkit designed to simplify data sourcing from a remote GraphQL API into Gatsby.",
  "author": "Vladimir Razuvaev <vladimir@gatsbyjs.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rimraf dist/* && tsc",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit",
    "format": "prettier --list-different --write ./src"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "gatsby",
    "graphql"
  ],
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "p-queue": "^6.4.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "gatsby": "^5.1.0",
    "graphql": "^16.6.0",
    "jest": "^29.3.1",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0 || ^4.0.0 || ^5.0.0"
  }
}
